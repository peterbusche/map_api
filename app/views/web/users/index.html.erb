<h1>Users</h1>
<ul>
  <% @users.each do |user| %>
    <li>
      <%= link_to user.username, web_user_path(user) %>
    </li>
  <% end %>
</ul>


<% if session[:user_id] %>
  <%= link_to "Upload New Image", new_web_user_image_path(session[:user_id]), class: "btn btn-success" %>
  <%= form_with url: web_logout_path, method: :delete, local: true do %>
    <%= submit_tag "Logout", class: "btn btn-danger" %>
  <% end %>

<% else %>
  <%= link_to "Create New User", new_web_user_path %>
  <%= link_to "Login", web_login_path, class: "btn btn-warning" %>
<% end %>